CREATE TABLE daily_vo2_max (
    day     TEXT NOT NULL PRIMARY KEY,
    vo2_max REAL
);

upsert:
INSERT OR REPLACE INTO daily_vo2_max(day, vo2_max)
VALUES (?, ?);

selectByDay:
SELECT * FROM daily_vo2_max WHERE day = ?;

selectInRange:
SELECT * FROM daily_vo2_max WHERE day >= ? AND day <= ? ORDER BY day ASC;

selectLatest:
SELECT * FROM daily_vo2_max WHERE day <= ? ORDER BY day DESC LIMIT 1;

selectPersonalBest:
SELECT MAX(vo2_max) AS personal_best FROM daily_vo2_max;
