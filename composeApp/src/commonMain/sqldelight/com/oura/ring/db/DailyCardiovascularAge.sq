CREATE TABLE daily_cardiovascular_age (
    day          TEXT NOT NULL PRIMARY KEY,
    vascular_age INTEGER
);

upsert:
INSERT OR REPLACE INTO daily_cardiovascular_age(day, vascular_age)
VALUES (?, ?);

selectByDay:
SELECT * FROM daily_cardiovascular_age WHERE day = ?;

selectInRange:
SELECT * FROM daily_cardiovascular_age WHERE day >= ? AND day <= ? ORDER BY day ASC;

selectLatest:
SELECT * FROM daily_cardiovascular_age WHERE day <= ? ORDER BY day DESC LIMIT 1;
